{
	"name": "hide-my-secrets",
	"displayName": "Hide My Secrets",
	"publisher": "emadashi",
	"description": "Hides secrets in YAML files",
	"icon": "128x128.png",
	"repository": {
		"url": "https://github.com/eashi/hide-my-secrets"
	},
	"version": "1.0.5",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hide-my-secrets.HideUnhideSecrets",
		"onLanguage:yaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hide-my-secrets.HideUnhideSecrets",
				"title": "Hide My Secrets: Toggle Hide"
			}
		],
		"configuration": {
			"title": "hide-my-secrets",
			"properties": {
				"hide-my-secrets.hide": {
					"type": "boolean",
					"default": true,
					"description": "Set this to true if you want to hide your secrets"
				},
				"hide-my-secrets.secretKeys": {
					"type": "array",
					"default": [
						"password",
						"connectionstring",
						"token"
					],
					"description": "List of keywords that whose values you want to hide"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"vsce": "1.79.3",
		"npm-version": "1.1.0"
	},
	"dependencies": {
		"yaml-ast-parser": "0.0.43"
	}
}
